<table id="report-{$idReport}-result-{$name}-table" class="resultTable name{ucfirst $name} sortable list">
	<thead>
		<tr class="header">
			{foreach $header head}
				<th class="col{ucfirst $head.name} {$head.class} {if $dwoo.foreach.default.last}last{/if}">{$head.label}</th>
			{/foreach}
		</tr>
	</thead>
	<tbody>
		{foreach $data row}
		<tr class="{cycle values="odd,even"}">
			{foreach $columnNames index columnName}
				<td class="colNum{$index} colName{ucfirst $columnName} {$header[$columnName][class]}">{$row[$columnName]}</td>
			{/foreach}
		</tr>
		{/foreach}
	</tbody>
</table>
{if $sortable !== false}
<script type="text/javascript">
Todoyu.Ext.reporting.ResultView.initTableSort('report-{$idReport}-result-{$name}', {json_encode($tableKitOptions)});
</script>
{/if}